class MyClass:
  i = 12345
  
  def f(self):
    return 'hello world'
  
x = MyClass()
print(x.f())


class Complex:
  def __init__(self, realpart, imagpart):
    self.r = realpart
    self.i = imagpart
  
  def hogehoge(self):
    print("hogehoge")
  
x = Complex(3, 4)
#print 3
print(x.r)

#print 4
print(x.i)
x.hogehoge()
